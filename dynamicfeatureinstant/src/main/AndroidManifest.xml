<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.dynamicfeatureinstant"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <dist:module
        dist:instant="true"
        dist:title="@string/text_instant_delivery">
        <dist:delivery>
            <dist:install-time />
        </dist:delivery>
        <dist:fusing dist:include="true" />
    </dist:module>
    <application>
        <activity
            android:name=".InstantDeliveryActivity"
            android:exported="true"
            android:label="Instant Delivery" />
    </application>
</manifest>